{
    "name": "wordpress-starter-kit",
    "version": "1.0.0",
    "description": "thePLAN Wordpress Starter Kit",
    "main": "/resources/js/app.js",
    "repository": "git@github.com:theplanworks/wordpress-starter-kit.git",
    "author": "Wilson Bridgett <wilson.bridgett@theplanworks.com>",
    "license": "MIT",
    "scripts": {
        "dev": "mix",
        "watch": "mix watch",
        "watch-poll": "mix watch -- --watch-options-poll=1000",
        "hot": "mix watch --hot",
        "production": "mix --production",
        "sync:up": "chmod +x ./code/scripts/sync.sh && ./code/scripts/sync.sh up",
        "sync:down": "chmod +x ./code/scripts/sync.sh && ./code/scripts/sync.sh down"
    },
    "devDependencies": {
        "cross-env": "^7.0.3",
        "laravel-mix": "^6.0.13",
        "postcss": "^8.1",
        "resolve-url-loader": "^3.1.2",
        "sass": "^1.32.8",
        "sass-loader": "10.*",
        "vue-template-compiler": "^2.6.7"
    }
}
